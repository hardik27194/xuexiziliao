<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bg"
    tools:context="com.liubowang.dubbing.Main.MainActivity">


    <ImageButton
        android:id="@+id/iv_set_main"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginRight="8dp"
        android:elevation="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <ImageButton
        android:id="@+id/ib_ad_main"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/iv_set_main"
        app:srcCompat="@mipmap/ic_launcher" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_v_50p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_h_40p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4" />

    <ImageView
        android:id="@+id/iv_icon_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_h_40p_main"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_h_90p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8983308" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_h_62p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.62" />

    <ImageButton
        android:id="@+id/ib_jianji_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorClean"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_12p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_44p_main"
        app:layout_constraintTop_toTopOf="@+id/gl_h_40p_main"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/shipin" />

    <ImageButton
        android:id="@+id/ib_xiaoyin_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorClean"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_56p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_88p_main"
        app:layout_constraintTop_toTopOf="@+id/gl_h_40p_main"
        app:layout_constraintVertical_bias="0.508"
        app:srcCompat="@drawable/shipin" />

    <ImageButton
        android:id="@+id/ib_hunyin_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorClean"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_12p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_44p_main"
        app:layout_constraintTop_toTopOf="@+id/gl_h_68p_main"
        app:srcCompat="@drawable/shipin" />

    <ImageButton
        android:id="@+id/ib_peiyin_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorClean"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_56p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_88p_main"
        app:layout_constraintTop_toTopOf="@+id/gl_h_68p_main"
        app:layout_constraintVertical_bias="0.484"
        app:srcCompat="@drawable/shipin" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/xiaoyin"
        android:textSize="15sp"
        android:padding="5dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_88p_main"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_h_62p_main"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_56p_main" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hunyin"
        android:textSize="15sp"
        android:padding="5dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_12p_main"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_h_90p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_44p_main"
        android:layout_marginRight="0dp"
        app:layout_constraintHorizontal_bias="0.538" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/peiyin"
        android:textSize="15sp"
        android:padding="5dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_56p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_88p_main"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_h_90p_main"
        app:layout_constraintHorizontal_bias="0.5" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:padding="5dp"
        android:text="@string/jianji"
        android:textAlignment="center"
        android:textSize="15sp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/gl_h_62p_main"
        app:layout_constraintLeft_toLeftOf="@+id/gl_v_12p_main"
        app:layout_constraintRight_toLeftOf="@+id/gl_v_44p_main"
        android:layout_marginRight="0dp" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_v_88p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.88" />

    <android.support.constraint.Guideline
        android:id="@+id/gl_v_12p_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.12" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gl_v_44p_main"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.44" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gl_v_56p_main"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.56" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gl_h_68p_main"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68" />


</android.support.constraint.ConstraintLayout>
